{
  "expo": {
    "name": "Konbini",
    "slug": "Konbini",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/konbini-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/konbini.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
