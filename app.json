      "adaptiveIcon": {
        "foregroundImage": "./assets/images/konbini.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trashbin_arduino.trashbin"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
